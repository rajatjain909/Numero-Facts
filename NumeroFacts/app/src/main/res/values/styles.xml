<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Retro application theme. -->
    <style name="RetroFitTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorRetroPrimary</item>
        <item name="colorPrimaryDark">@color/colorRetroPrimaryDark</item>
        <item name="colorAccent">@color/colorRetroAccent</item>
    </style>

</resources>
